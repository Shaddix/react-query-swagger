{%- if operation.ContentParameter.IsArray -%}
{%- assign typeWithoutBrackets = operation.ContentParameter.Type |  prepend: '  ' | slice: 0, operation.ContentParameter.Type.size | lstrip -%}
{{ operation.ContentParameter.VariableName }} = {{ operation.ContentParameter.VariableName }}.map(item => Types.prepareSerialize{{typeWithoutBrackets}}(item))
const content_ = JSON.stringify({{ operation.ContentParameter.VariableName }});
{%- elsif operation.ContentParameter.IsObject -%}
const content_ = Types.serialize{{ operation.ContentParameter.Type }}({{ operation.ContentParameter.VariableName }});
{%- else -%}
const content_ = JSON.stringify({{ operation.ContentParameter.VariableName }});
{%- endif -%}